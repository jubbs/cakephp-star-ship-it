{
    "name": "jubbs/cakephp-star-ship-it",
    "description": "StarShipIt plugin for CakePHP4",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "cakephp/cakephp": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "StarShipIt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StarShipIt\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}